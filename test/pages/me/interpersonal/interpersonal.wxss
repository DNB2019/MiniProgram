page{
    background-color: #E4F9FD;
}
.avartar image{
    width:392rpx;
    height: 392rpx;
    margin: auto;
}
.type{
    background-color: #A5D5D5;
    color: white;
    width: 240rpx;
    height: 70rpx;
    line-height: 60rpx;
    border:4px solid rgba(255,255,255,1);
    text-align: center;
    border-radius: 40rpx;
    margin: auto;
}
.test-bar{
    width:662rpx;
    height: 136rpx;
    margin:30rpx auto 10rpx auto;
}
.test{
    width: 136rpx;
    height: 136rpx;
    margin:0rpx 15rpx;
}
.test-text{
    height: 50rpx;
    width: 662rpx;
    margin: auto;
    position: relative;
    top:-10rpx;
}
.test-text text{
    display: inline-block;
    width: 136rpx;
    line-height: 56rpx;
    font-size: 28rpx;
    color:#418A8A;
    margin-left: 25rpx;
    margin-right: 5rpx;
}
.finish-bar{
    width: 662rpx;
    height: 30rpx;
    margin:auto;
}
.status{
    width: 30rpx;
    height: 30rpx;
    margin: 0rpx 66rpx;
}
.report{
    width: 670rpx;
    height: 580rpx;
    border-radius:10rpx;
    background-color: white;
    padding-top:180rpx;
    margin:50rpx auto 100rpx auto;
}
.report text{
    width: 300rpx;
    font-size: 36rpx;
    text-align: center;
    line-height: 56rpx;
    display: block;
    margin:auto;
}
.progress{
    width:500rpx;
    height: 20rpx;
    border-radius: 10rpx;
    background-color: rgb(236, 236, 236);
    margin:30rpx auto;
    display: flex;
}
.fin-progress{
    display: inline-block;
    width:0rpx;
    height: 20rpx;
    border-radius: 10rpx;
    background-color: #A5D5D5;
}
.progress-num{
    font-size: 36rpx;
    line-height: 56rpx;
    color: #A5D5D5;
    text-align: center;
}
